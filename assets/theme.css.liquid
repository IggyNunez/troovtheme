/*================ Typography ================*/
{%- if settings.type_base_family contains 'Google' -%}
  {%- assign type_base_parts = settings.type_base_family | split: '_' -%}
  {%- assign type_base_name = type_base_parts[1] -%}
  {% capture font_base_stack %}"{{ type_base_name | split: ':' | first | replace: '+', ' ' }}"{% if type_base_parts.last == 'serif' %}, serif {% else %}, 'HelveticaNeue', 'Helvetica Neue', sans-serif{% endif %}{% endcapture %}
{%- else -%}
  {%- assign font_handle = settings.type_base_family -%}
  @font-face {
    font-family: "{{ settings.type_base_family }}";
    src: url("{{ font_handle | append: '.woff2' | asset_url }}") format("woff2"), url("{{ font_handle | append: '.woff' | asset_url }}") format("woff");
  }
  {%- assign font_base_stack = settings.type_base_family | prepend: "'" | append: "', 'Helvetic Neue', 'Helvetica', san-serif" -%}
  {%- assign font_base_weight = 300 -%}

{%- endif -%}

{%- if settings.type_header_family contains 'Google' -%}
  {%- assign type_header_parts = settings.type_header_family | split: '_' -%}
  {%- assign type_header_name = type_header_parts[1] -%}
  {% capture font_header_stack %}"{{ type_header_name | split: ':' | first | replace: '+', ' ' }}"{% if type_header_parts.last == 'serif' %}, serif {% else %}, 'HelveticaNeue', 'Helvetica Neue', sans-serif{% endif %}{% endcapture %}
  {%- assign type_weight_part = type_header_parts[2] | split: ';' -%}
  {%- assign font_header_weight = type_weight_part[0] | default: type_weight_part -%}
{%- else -%}
  {%- assign font_handle = settings.type_header_family -%}
  @font-face {
  	font-family: "{{ settings.type_header_family }}";
  	src: url("{{ font_handle | append: '.woff' | asset_url }}") format("woff"), url("{{ font_handle | append: '.woff2' | asset_url }}") format("woff2");
  }
  {%- assign font_header_stack = settings.type_header_family | prepend: "'" | append: "', 'Helvetic Neue', 'Helvetica', san-serif" -%}
  {%- assign font_header_weight = 700 -%}
{%- endif -%}

{%- if settings.type_title_family contains 'Google' -%}
  {%- assign type_title_parts = settings.type_title_family | split: '_' -%}
  {%- assign type_title_name = type_title_parts[1] -%}
  {% capture font_title_stack %}"{{ type_title_name | split: ':' | first | replace: '+', ' ' }}"{% if type_titler_parts.last == 'serif' %}, serif {% else %}, 'HelveticaNeue', 'Helvetica Neue', sans-serif{% endif %}{% endcapture %}
{%- else -%}
  {%- assign font_handle = settings.type_title_family -%}
  @font-face {
  	font-family: "{{ settings.type_title_family }}";
  	src: url("{{ font_handle | append: '.woff' | asset_url }}") format("woff"), url("{{ font_handle | append: '.woff2' | asset_url }}") format("woff2");
  }
  {%- assign font_title_stack = settings.type_title_family | prepend: "'" | append: "', 'Helvetic Neue', 'Helvetica', san-serif" -%}
  {%- assign font_title_weight = 700 -%}
{%- endif -%}

{%- if settings.type_accent_family contains 'Google' -%}
  {%- assign type_accent_parts = settings.type_accent_family | split: '_' -%}
  {%- assign type_accent_name = type_accent_parts[1] -%}
  {% capture font_accent_stack %}"{{ type_accent_name | split: ':' | first | replace: '+', ' ' }}"{% if type_nav_parts.last == 'serif' %}, serif {% else %}, 'HelveticaNeue', 'Helvetica Neue', sans-serif{% endif %}{% endcapture %}
  {%- assign font_accent_weight = type_accent_parts[2] | split: ',' -%}
{%- elsif settings.type_accent_family != blank -%}
  {%- assign font_handle = settings.type_accent_family -%}
  @font-face {
  	font-family: "{{ settings.type_accent_family }}";
  	src: url('{{ font_handle | append: '.woff' | asset_url }}'), url('{{ font_handle | append: '.woff2' | asset_url }}') format("woff2");
  }
  {%- assign font_accent_stack = settings.type_accent_family | prepend: "'" | append: "', 'Helvetic Neue', 'Helvetica', san-serif"  -%}
  {%- assign font_accent_weight = 400 -%}
{%- endif -%}

