mmation \n<\/p>\n<\/li>\n<li> <p><strong> 1590mg maca root <\/strong> for improved stamina, strength, and mood.\n<\/p>\n<\/li>\n<\/ul>","body_size":0,"body_size_xs":0,"color_body":"rgba(0,0,0,0)","cta_link":"","cta_label":"","cta_theme":"btn-primary","cta_size":"","show_ingredients":false}},"eca68c83-62c2-472c-8ea8-cb529a9cdbe1":{"type":"image","settings":{"color_bg":"rgba(0,0,0,0)","image":"shopify:\/\/shop_images\/Troov-Energy-NFP-IngredientList.gif","image_width":325,"alignment":"text-center","cta_link":""}}},"block_order":["7e8d37c3-7ca0-4375-8f0c-40a8f87a6ac3","eca68c83-62c2-472c-8ea8-cb529a9cdbe1"],"disabled":true,"settings":{"fluid_section":false,"color_bg":"rgba(0,0,0,0)","anchor_id":"whatsinside","class":"","v_align":"align-items-center","h_align":"justify-content-center","no_gap":false,"grid":2,"grid_xs":1,"mobile_assets_first":true,"show_carousel":"","slides_to_show":2,"carousel_arrows":true,"carousel_dots":false,"header_alignment":"text-center","header_title":"What's inside","header_title_tag":"h2","display_class":"display-2","header_color_title":"rgba(0,0,0,0)","header_body":"","header_color_body":"rgba(0,0,0,0)","header_cta_link":"","header_cta_label":"","header_cta_theme":"btn-primary","header_cta_size":"","footer_alignment":"","footer_title":"","footer_title_tag":"h2","footer_color_title":"rgba(0,0,0,0)","footer_body":"","footer_color_body":"rgba(0,0,0,0)","footer_cta_link":"","footer_cta_label":"","footer_cta_theme":"btn-primary","footer_cta_size":""}},"1660857685f9ca8aa4":{"type":"multicolumn","blocks":{"5d5debf2-4bd8-457f-8eb9-b3338b79b8d9":{"type":"image","settings":{"color_bg":"rgba(0,0,0,0)","image":"shopify:\/\/shop_images\/06.png","image_width":800,"alignment":"text-center","cta_link":""}}},"block_order":["5d5debf2-4bd8-457f-8eb9-b3338b79b8d9"],"disabled":true,"settings":{"fluid_section":false,"color_bg":"rgba(0,0,0,0)","anchor_id":"","class":"","v_align":"align-items-center","h_align":"justify-content-center","no_gap":false,"grid":1,"grid_xs