      "multicolumn_TegNkM": {
        "type": "multicolumn",
        "blocks": {
          "html_ybctdr": {
            "type": "html",
            "settings": {
              "color_bg": "rgba(0,0,0,0)",
              "html": "<div class=\"email-signup-container\" style=\"max-width: 500px; margin: 0 auto; padding: 30px; text-align: center; background-color: #f8f8f8; border-radius: 8px;\">\n  <h2 style=\"margin-bottom: 15px; color: #333;\">We're Currently Out of Stock</h2>\n  <p style=\"margin-bottom: 20px; color: #666;\">Sign up to be notified when our products are back in stock and ready to ship!</p>\n  \n  <div id=\"signup-form-container\">\n    <form id=\"email-signup-form\" action=\"/contact#contact_form\" method=\"post\" accept-charset=\"UTF-8\" class=\"contact-form\" onsubmit=\"showThankYouMessage(); return true;\">\n      <input type=\"hidden\" name=\"form_type\" value=\"customer\">\n      <input type=\"hidden\" name=\"utf8\" value=\"✓\">\n      <input type=\"hidden\" name=\"contact[tags]\" value=\"out-of-stock\">\n      \n      <div style=\"display: flex; max-width: 400px; margin: 0 auto; flex-wrap: wrap;\">\n        <input type=\"email\" name=\"contact[email]\" placeholder=\"Your email address\" required style=\"flex: 1; min-width: 200px; padding: 12px; border: 1px solid #ddd; border-radius: 4px; font-size: 16px; margin-bottom: 10px;\">\n        <button type=\"submit\" style=\"background-color: #4CAF50; color: white; border: none; padding: 12px 20px; border-radius: 4px; cursor: pointer; font-size: 16px; width: 100%;\">Notify Me</button>\n      </div>\n      \n      <p style=\"margin-top: 15px; font-size: 12px; color: #999; max-width: 400px; margin-left: auto; margin-right: auto;\">We respect your privacy and will never share your information.</p>\n    </form>\n  </div>\n  \n  <div id=\"thank-you-message\" style=\"display: none; margin-top: 20px;\">\n    <div style=\"background-color: #e8f5e9; padding: 15px; border-radius: 4px; border-left: 4px solid #4CAF50;\">\n      <h3 style=\"color: #2E7D32; margin-bottom: 10px;\">Thank You!</h3>\n      <p style=\"color: #388E3C;\">Your submission has been completed. We'll notify you when our products are back in stock.</p>\n    </div>\n  </div>\n</div>\n\n<script>\n  function showThankYouMessage() {\n    // Store that the form was submitted in session storage\n    sessionStorage.setItem('formSubmitted', 'true');\n  }\n  \n  // Check if form was submitted when page loads\n  document.addEventListener('DOMContentLoaded', function() {\n    if (sessionStorage.getItem('formSubmitted') === 'true') {\n      document.getElementById('signup-form-container').style.display = 'none';\n      document.getElementById('thank-you-message').style.display = 'block';\n      // Clear the session storage after showing the message\n      setTimeout(function() {\n        sessionStorage.removeItem('formSubmitted');\n      }, 5000);\n    }\n  });\n</script>"
            }
          }
